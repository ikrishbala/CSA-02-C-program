#include <stdio.h>
#include <stdlib.h>

int minJumps(int arr[], int n)
{
    int *jumps = (int*)malloc(sizeof(int) * n); 
    int i, j;

    if (n == 0 || arr[0] == 0) { 
        return -1;
    }

    jumps[0] = 0; 
    for (i = 1; i < n; i++) {
        jumps[i] = INT_MAX;
        for (j = 0; j < i; j++) {
            if (i <= j + arr[j] && jumps[j] != INT_MAX) {
                jumps[i] = jumps[j] + 1;
                break; 
            }
        }
    }

    if (jumps[n - 1] == INT_MAX) { 
        return -1;
    } else {
        return jumps[n - 1];
    }
}

int main()
{
    int arr[] = {3, 4, 2, 1, 2, 1};
    int n = sizeof(arr) / sizeof(arr[0]);
    int min_jumps = minJumps(arr, n);

    printf("Minimum number of jumps to reach the end of the array starting from index 0: %d", min_jumps);

    return 0;
}
