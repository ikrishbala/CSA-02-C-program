#include <stdio.h>

int isComposite(int n) {
    int i;
    if(n<2)
        return 0;
    for(i=2;i<=n/2;i++) {
        if(n%i==0)
            return 1;
    }
    return 0;
}

int main() {
    int i;
    printf("Composite numbers between 1 and 50 are: ");
    for(i=1;i<=50;i++) {
        if(isComposite(i)) {
            printf("%d ", i);
        }
    }
    return 0;
}
